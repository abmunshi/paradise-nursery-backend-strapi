{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Categories"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "minLength": 3,
      "required": true,
      "unique": true
    },
    "plants": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::plant.plant",
      "mappedBy": "category"
    }
  }
}
